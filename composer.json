{
    "name": "nickcheek/privacy-wrapper",
    "description": "PHP Api wrapper for Privacy API",
    "keywords": [
        "nickcheek",
        "privacy-wrapper"
    ],
    "homepage": "https://github.com/nickcheek/privacy-wrapper",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Nicholas Cheek",
            "email": "nick@nicholascheek.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Nickcheek\\PrivacyWrapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nickcheek\\PrivacyWrapper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
